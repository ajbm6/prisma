{
    "name": "odan/psr7-full-stack",
    "type": "project",
    "description": "PSR-7 Full-Stack framework",
    "keywords": ["framework", "psr-7", "middleware"],
    "homepage": "http://github.com/odan/psr7-full-stack",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6 || ^7.0",
        "zendframework/zend-diactoros": "^1.3",
        "relay/relay": "^1.1",
        "nikic/fast-route": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "App\\Test\\": "tests"
        }
    }
}
