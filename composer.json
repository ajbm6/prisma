        "nikic/fast-route": "^1.0",
        "monolog/monolog": "^1.18.0",
        "symfony/http-foundation": "~3.0",
        "symfony/translation": "~3.0",
        "cakephp/utility": "^3.2.10",
        "cakephp/database": "^3.2.10",
        "robmorgan/phinx": "0.5.*",
        "league/plates": "3.*",
        "odan/plates-asset-cache": "0.1.*"
